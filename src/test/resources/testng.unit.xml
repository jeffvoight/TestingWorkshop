<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Unit Suite" verbose="1">
    <groups>
        <run>
            <exclude name="windows"/>
            <exclude name="macos"/>
            <exclude name="android"/>
            <exclude name="ios"/>
            <exclude name="ui"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.afs.tas.listeners.SuiteListener"/>
        <listener class-name="com.afs.tas.listeners.TestListener"/>
    </listeners>
    <test name="Regression">
        <packages>
            <package name="gov.dhs.tsa.tas.*">
                <exclude name="gov.dhs.tsa.tas.devices"/>
            </package>
        </packages>
    </test>
</suite>
