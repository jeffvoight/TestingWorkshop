<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Integration Suite" verbose="1">
    <groups>
        <run>
            <exclude name="windows"/>
            <exclude name="macos"/>
            <exclude name="android"/>
            <exclude name="ios"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="gov.dhs.tsa.tas.listeners.SuiteListener"/>
        <listener class-name="gov.dhs.tsa.tas.listeners.UITestListener"/>
    </listeners>
    <test name="Devices">
        <packages>
            <package name="gov.dhs.tsa.tas.devices.*"/>
        </packages>
    </test>

</suite>
